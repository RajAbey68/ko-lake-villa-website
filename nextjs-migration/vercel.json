{
  "version": 2,
  "name": "ko-lake-villa",
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "regions": ["sin1"],
  "env": {
    "NEXT_PUBLIC_SITE_URL": "https://kolakevilla.com",
    "ANALYTICS_ID": "@analytics_id"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  }
}